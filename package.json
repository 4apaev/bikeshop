{
  "name": "bikeshop",
  "version": "0.1.1",
  "description": "Cooperative bicycle workshop",
  "author": "Michael Miretski",
  "license": "ISC",
  "type": "module",
  "eslintIgnore": [
    "**/*.ts",
    "node_modules"
  ],

  "repository": {
    "type": "git",
    "url": "git@github.com:4apaev/bikeshop.git"
  },

  "scripts": {
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "start": "npm run migrate && node wheels/koa.js",
    "migrate": "npm run down && npm run up && sleep 3 && node db/migrate.js",
    "test": "c8 --check-coverage mocha ./tests/*.spec.js",
    "lint": "eslint --fix src",
    "mocha": "mocha",
    "types": "tsc src/**/*.js"
  },

  "dependencies": {
    "pg": "8.7.1",
    "koa": "2.13.4",
    "sync": "git@github.com:4apaev/sync.git"
  },
  "devDependencies": {
    "@types/pg": "8.6.3",
    "@types/koa": "2.13.4",
    "@types/node": "17.0.5",
    "@babel/core": "7.16.5",
    "@babel/eslint-parser": "7.16.5",
    "@babel/eslint-plugin": "7.16.5",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "eslint": "8.5.0",
    "mocha": "9.1.3",
    "c8": "7.10.0"
  }
}



