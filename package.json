    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.17.0",
    "@babel/eslint-plugin": "7.17.7",
    "@types/koa"    : "2.13.4",
    "@types/node"   : "17.0.33",
    "@types/pg"     : "8.6.5",
    "@types/stylus" : "0.48.37",
    "c8"            : "7.11.2",
    "eslint"        : "8.15.0",
    "mocha"         : "10.0.0"
  }
}