{
  "name": "bikeshop",
  "version": "0.1.1",
  "description": "Cooperative bicycle workshop",
  "author": "Michael Miretski",
  "license": "ISC",
  "type": "module",
  "eslintIgnore": [
    "**/*.ts",
    "**/*.svg",
    "node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:4apaev/bikeshop.git"
  },
  "scripts": {
    "up": "docker-compose up -d",
    "down": "docker-compose down",
    "start": "npm run down && npm run up && sleep 3 && npm run wheels",
    "wheels": "node index.js",
    "migrate": "node db/migrate.js",
    "test": "c8 --check-coverage mocha ./tests/*.spec.js",
    "lint": "eslint --fix src",
    "mocha": "mocha",
    "types": "tsc src/**/*.js"
  },
  "dependencies": {
    "koa": "^2.13.4",
    "pg": "^8.7.1",
    "stylus": "^0.56.0"
  },
  "devDependencies": {
    "c8": "^7.11.0",
    "mocha": "^9.1.4",
    "eslint": "^8.7.0",
    "@babel/core": "^7.16.10",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/eslint-plugin": "^7.16.5",
    "@types/node": "^17.0.10",
    "@types/koa": "^2.13.4",
    "@types/pg": "^8.6.4"
  }
}
